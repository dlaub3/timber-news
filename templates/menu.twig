
{% if menu %}

	<ul class="menu small-12 medium-5 columns dropdown" role="menubar" data-dropdown-menu="qovrot-dropdown-menu">
	{% for item in menu %}
		<li class="is-submenu-item is-dropdown-submenu-item {{item.classes | join(' ')}} ">
			<a href="{{item.get_link}}">{{item.title}}</a>
			{% include "menu.twig" with {'menu': item.get_children} %}
		</li>
	{% endfor %}
	</ul>

{% endif %}
